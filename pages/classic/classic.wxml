<view class="container">
  <view class="header">
    <yp-episode class="episode" index="{{classic.index}}"></yp-episode>
    <yp-like class="like" count="{{classic.fav_nums}}" likeFlag="{{classic.like_status}}" bind:like='onLike'></yp-like>
  </view>
  <yp-movie img="{{classic.image}}" content="{{classic.content}}"></yp-movie>
  <yp-navi class="navi" title="{{classic.title}}" latest="{{latest}}" first="{{first}}" bind:prePage='pre' bind:nextPage='next'></yp-navi>
</view>